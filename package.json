{
  "name": "Ironbelly",
  "version": "0.0.3",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "./node_modules/.bin/eslint src/**/*.js"
  },
  "hooks": {
    "pre-commit": "npm run lint"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "apsl-react-native-button": "^3.1.1",
    "bugsnag-react-native": "^2.14.0",
    "flux-standard-action": "^2.0.3",
    "moment": "^2.23.0",
    "react": "16.6.1",
    "react-native": "^0.57.7",
    "react-native-fs": "^2.13.3",
    "react-native-gesture-handler": "^1.0.12",
    "react-native-swipe-list-view": "^1.5.0",
    "react-native-toaster": "^1.2.2",
    "react-navigation": "^3.0.8",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-react-native": "^5",
    "bugsnag-sourcemaps": "^1.1.0",
    "detox": "^10.0.12",
    "enzyme": "^3.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.92.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-serializer": "^24.0.0",
    "prettier": "^1.16.4",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.5.0",
    "react-test-renderer": "16.4.1"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/",
      "./src/assets/images/"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/Ironbelly.app",
        "build": "xcodebuild -workspace ios/Ironbelly.xcworkspace -scheme Ironbelly -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone SE"
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/Ironbelly.app",
        "build": "xcodebuild -workspace ios/Ironbelly.xcworkspace -scheme Ironbelly -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone SE"
      }
    },
    "test-runner": "jest"
  }
}
